<h1>Welcome New User</h1>
<%= form_for (@user) do |s| %>

<%= render partial: 'layouts/errors', locals: {object: @user} %>


<%= s.label :username %>
<%= s.text_field :username %>
<br>
<%= s.label :email %>
<%= s.email_field :email %>
<br>
<%= s.label :password %>
<%= s.password_field :password %>
<%= s.submit "SignUp" %>
<% end %>